{
	"name": "xp-datalayer-manager",
	"version": "1.0.0",
	"description": "A multi-framework utility package for managing XP Data Layer events",
	"module": "dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./react": {
			"import": "./dist/react/index.js",
			"require": "./dist/react/index.js",
			"types": "./dist/react/index.d.ts"
		},
		"./svelte": {
			"import": "./dist/svelte/index.js",
			"require": "./dist/svelte/index.js",
			"types": "./dist/svelte/index.d.ts"
		},
		"./angular": {
			"import": "./dist/angular/index.js",
			"require": "./dist/angular/index.js",
			"types": "./dist/angular/index.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"react": [
				"dist/react/index.d.ts"
			],
			"svelte": [
				"dist/svelte/index.d.ts"
			],
			"angular": [
				"dist/angular/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun build ./src/index.ts ./src/react/index.ts ./src/svelte/index.ts ./src/angular/index.ts --outdir ./dist --target browser",
		"build:types": "tsc --emitDeclarationOnly",
		"dev": "bun --watch src/index.ts",
		"test": "bun test",
		"prepare": "bun run build && bun run build:types"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"svelte": ">=3.0.0",
		"@angular/core": ">=13.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"svelte": {
			"optional": true
		},
		"@angular/core": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/react": "^18.2.0",
		"@angular/core": "^13.0.0",
		"typescript": "^5.0.0",
		"bun-types": "latest",
		"svelte": "^3.0.0",
		"react": "^18.2.0"
	},
	"keywords": [
		"adobe",
		"data-layer",
		"analytics",
		"react",
		"svelte",
		"angular"
	],
	"author": {
		"name": "Jordan Robinson",
		"email": "hello.world@jordanrobo.xyz"
	},
	"license": "MIT"
}
